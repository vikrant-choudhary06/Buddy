# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_bot_token

# Database
MONGODB_URI=your_mongodb_uri

# Environment
ENVIRONMENT=development
# Optional privileged intents (true/false)
# These must also be enabled in Discord Developer Portal > Bot > Privileged Gateway Intents
DISCORD_INTENT_MESSAGE_CONTENT=true
DISCORD_INTENT_MEMBERS=true
DISCORD_INTENT_PRESENCES=true
